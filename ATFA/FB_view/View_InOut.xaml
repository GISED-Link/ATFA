             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ATFA.FB_view"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="200">
    <Grid x:Name="InGrid">

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>
        <!--
        <Rectangle Grid.Column="0" Fill="Black" Width="25" Height="5" 
				  VerticalAlignment="Center" />
        -->
        <Label x:Name="Text" Content="Place holder input 1" Grid.Column="1"
			   HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="16" />
    </Grid>
</UserControl>
